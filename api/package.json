{
  "name": "triggmine-ico",
  "version": "0.10.0",
  "description": "Boilerplate project for a restful node backend server powered by ES6 and Express",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && babel app -s -D -d build",
    "clean": "rimraf ./build && mkdir build",
    "start": "nodemon",
    "test": "NODE_ENV=test mocha --recursive --reporter spec --compilers js:babel-register tests",
    "lint": "eslint app",
    "test:watch": "nodemon --exec 'npm run test' tests",
    "coverage": "NODE_ENV=test babel-node ./node_modules/.bin/isparta cover _mocha -- --recursive --reporter spec --compilers js:babel-register tests",
    "validate": "npm run test && npm outdated --depth 0",
    "build-css": "node-sass --include-path scss ../css/style.scss ../css/style.css"
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kylealwyn/node-rest-api-boilerplate.git"
  },
  "keywords": [
    "express",
    "es6",
    "rest",
    "api",
    "boilerplate",
    "mongo"
  ],
  "author": "Kyle Alwyn <kjalwyn@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kylealwyn/node-rest-api-boilerplate/issues"
  },
  "homepage": "https://github.com/kylealwyn/node-rest-api-boilerplate",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "bn.js": "^4.11.8",
    "body-parser": "^1.15.2",
    "buffer-slice": "0.0.1",
    "build-url": "^1.0.10",
    "cors": "^2.8.1",
    "crypto": "^1.0.1",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "ethereumjs-tx": "^1.3.3",
    "ethereumjs-util": "^5.1.2",
    "ethereumjs-wallet": "^0.6.0",
    "express": "^4.15.4",
    "googleplaces": "^0.6.1",
    "handlebars": "^4.0.10",
    "helmet": "^3.1.0",
    "http-status": "^1.0.1",
    "jade": "^1.11.0",
    "joi": "^13.1.1",
    "jsonwebtoken": "^7.1.0",
    "kissmetrics-js": "^0.4.0",
    "lodash": "^4.16.4",
    "method-override": "^2.3.6",
    "moment": "^2.18.1",
    "momentjs": "^2.0.0",
    "mongoose": "^5.0.3",
    "mongoose-unique-validator": "^2.0.0",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "node-geocoder": "^3.22.0",
    "nodemailer": "^4.1.0",
    "percentage-calc": "0.0.1",
    "pug": "^2.0.0-rc.3",
    "recaptcha2": "^1.3.2",
    "rimraf": "^2.6.1",
    "tracer": "^0.8.11",
    "validator": "^9.4.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.8.1",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-babel": "^4.0.0",
    "faker": "^3.1.0",
    "isparta": "^4.0.0",
    "mocha": "^3.1.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "request": "^2.81.0"
  }
}
